{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://pathways.rmi.org/schema/common/emissionsScope.v1.json",
  "title": "Emissions Scope",
  "description": "Defines which greenhouse gases are covered in the pathway's modeled emissions.",
  "type": "string",
  "enum": [
    "No information",
    "CO2",
    "CO2e (Kyoto)",
    "CO2e (CO2, Methane)",
    "CO2e (unspecified GHGs)",
    "Other emissions scope"
  ]
}
